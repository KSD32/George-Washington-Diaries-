      <schemaSpec ident="starterODD" start="TEI" prefix="tei">
        <moduleRef key="tei"/>
        <moduleRef key="core" except="binaryObject cb distinct divGen gb headItem headLabel index lb mentioned monogr postbox postCode sp speaker stage"/>
        <moduleRef key="header"/> 
        <moduleRef key="linking" include="seg"/>
        <moduleRef key="namesdates"/>
        <moduleRef key="textstructure" include="TEI body closer dateline div opener postscript salute signed text"/>
        <constraintSpec ident="letterDates" scheme="schematron">
          <constraint>
            <sch:rule context="tei:dateline">
              <sch:report test="matches(., '\d,\s*\d{4}')">Date element missing!! Please wrap the date in date element</sch:report>
            </sch:rule>
          </constraint>
        </constraintSpec>
        
